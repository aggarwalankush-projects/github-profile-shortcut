{
  "name": "Github Profile Shortcut",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "Adds github profile button in header",
  "homepage_url": "https://github.com/aggarwalankush-projects/github-profile-shortcut",
  "icons": {
    "16": "icons/icon16.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
